    <!-- Inline JavaScript for Counter Animation -->
    <script>
        document.querySelectorAll('.counter').forEach(counter => {
            const updateCounter = () => {
                const target = +counter.innerText; // Get the target number
                let count = 0;
                const increment = target / 200; // Increment step (adjust speed)
                const interval = setInterval(() => {
                    count += increment;
                    if (count >= target) {
                        count = target;
                        clearInterval(interval);
                    }
                    counter.innerText = Math.ceil(count);
                }, 10); // Speed of the animation
            };
            updateCounter();
        });
    </script>
